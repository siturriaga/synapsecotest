{
  "name": "synapse-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.18"
  },
  "scripts": {
    "api:canary": "node scripts/api-canary.cjs",
    "build": "vite build",
    "build:tests": "tsc -p tsconfig.test.json",
    "check:env": "node scripts/check-env.cjs",
    "check:paths": "node scripts/check-paths.cjs",
    "check:routes": "node scripts/check-routes.cjs",
    "contract:check": "node scripts/contract-check.cjs",
    "dev": "vite",
    "diagnostics": "npm run build && npm run contract:check && npm run api:canary && npm run scan:secrets && npm test && node scripts/bundle-report.cjs && node scripts/verify-routing.cjs",
    "preview": "vite preview",
    "scan:secrets": "node scripts/secrets-scan.cjs",
    "simulate:ai": "node scripts/simulate-ai.cjs",
    "start": "vite preview",
    "test": "node --test tests/**/*.test.mjs"
  },
  "dependencies": {
    "busboy": "^1.6.0",
    "exceljs": "^4.4.0",
    "firebase": "^11.6.1",
    "firebase-admin": "^12.6.0",
    "mammoth": "^1.6.0",
    "pdf-parse": "^1.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "wouter": "^3.2.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.157",
    "@types/busboy": "^1.5.4",
    "@types/node": "^24.9.2",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "^5.6.3",
    "vite": "^7.1.12"
  }
}
